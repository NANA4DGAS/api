def scan_website(url):
    try:
        startBrute(url)
        checkleaf(url)
        checkkrocotessatu(url)
        checkkrocotesdua(url)
        checkkrocotestiga(url)
        checkkrocotesempat(url)
        checkkroco(url)
        checkkrocodua(url)
        checkkrocotiga(url)
        checkkrocoempat(url)
        checkkrocolima(url)
        checkkrocoenam(url)
        checkkrocotujuh(url)
        checkkrocodelapan(url)
        checkkrocosembilan(url)
        checkkrocosepuluh(url)
        checkkrocosebelas(url)
