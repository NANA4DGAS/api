def checkleaf(url):
    try:
        url_http = 'http://' + code_shell.url_domain(url)

        check = requests.get(url_http + '/xmrlpc.php7', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
        if 'Uname:' in check.text or 'Yanz Webshell!' in check.text:
            print(' -| {:<50} --> {}[Succesfully] [wso]'.format(url_http, fg))
            open('results/Yanz Webshell!.txt', 'a').write(url_http + '/xmrlpc.php7\n')           

        check = requests.get(url_http + '/lock360.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
