def checpath(site):
    try:
        url = "http://" + code_shell.url_domain(site)
        for Path in Path_plugins:
            response = requests.get(url + Path, headers=code_shell.headers, verify=False, timeout=15)
            if response.status == 200:
                print(' -| {:<50} --> {}[Succesfully] [path]'.format(url, fg))
                open('results/path.txt', 'a').write(url + Path + "\n")
                break
            else:
                print(' -| {:<50} --> {}[Failed]'.format(site, fr))
    except:
        pass

