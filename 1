import re
import os
import sys

try:
    from colorama import Fore, init
    import requests
    from pystyle import *
except Exception as e:
    import sys, os

    os.system('cls' if os.name == 'nt' else 'clear')
    print("[!] Modul belum di-install!")
    print("    Mencoba install otomatis...\n")

    install_cmds = [
        'pip install requests colorama pystyle',
        'py -m pip install requests colorama pystyle',
        'python -m pip install requests colorama pystyle',
        f'"{sys.executable}" -m pip install requests colorama pystyle'
    ]

    success = False
    for cmd in install_cmds:
        print(f"[~] Coba: {cmd}")
        code = os.system(cmd)
        if code == 0:
            success = True
            break

    if not success:
        print("\n[!] Gagal install otomatis.")
        print("    Jalankan manual:")
        print("    python -m pip install requests colorama pystyle")
        sys.exit(1)

    print("\n[✓] Install selesai. Restart script...\n")
    os.execv(sys.executable, [sys.executable] + sys.argv)


