def checkrandom1(url):
    try:
        url_http = 'http://' + code_shell.url_domain(url)
     
        check = requests.get(url_http + '/simple.php', headers=code_shell.headers, allow_redirects=True, timeout=15)
        if '{Ninja-Shell}' in check.text or '<title>Simple File Manage Design by index.php</title>' in check.text or "title: 'Are you sure?'," in check.text or '<b>Yanz Webshell!</b>' in check.text or 'Mr.Combet' in check.text or '-rw-r--r--' in check.text:
            print(' -| {:<50} --> {}[Succesfully] [ninja]'.format(url, fg))
